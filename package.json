{
  "name": "react-webinar-3",
  "version": "1.0.0",
  "description": "Example project for webinar",
  "type": "module",
  "scripts": {
    "start": "vite serve",
    "preview": "vite preview",
    "dev": "nodemon",
    "prod": "cross-env NODE_ENV=production node --no-warnings=ExperimentalWarning --loader ts-node/esm server/index.ts",
    "build:client": "vite build",
    "build:server": "vite build --ssr entry-server.tsx",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:pw": "npx playwright test",
    "test:ui": "npx playwright test --ui",
    "tscheck": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ylabio/react-webinar-3.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ylabio/react-webinar-3/issues"
  },
  "homepage": "https://github.com/ylabio/react-webinar-3#readme",
  "dependencies": {
    "@ant-design/charts": "^2.0.4",
    "@ant-design/icons": "^5.3.6",
    "@ant-design/plots": "^2.1.16",
    "@bem-react/classname": "^1.6.0",
    "@vitejs/plugin-react": "^4.2.1",
    "antd": "^5.16.2",
    "compression": "^1.7.4",
    "dayjs": "^1.11.10",
    "dompurify": "^3.1.0",
    "emoji-picker-react": "^4.9.2",
    "express": "^4.19.2",
    "http-proxy-middleware": "^3.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.1.0",
    "react-redux": "^9.1.1",
    "react-router-dom": "^6.22.3",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "shallowequal": "^1.1.0",
    "sirv": "^2.0.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
