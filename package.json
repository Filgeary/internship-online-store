{
  "name": "ylab-react-internship",
  "version": "1.0.0",
  "description": "Example project for Ylab React Internship",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node --loader ts-node/esm server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "cross-env NODE_ENV=production node --loader ts-node/esm server",
    "test": "jest",
    "format": "prettier . --write",
    "lint:prettier": "prettier . --check",
    "lint:eslint": "eslint \"**/**.{js,ts,jsx,tsx}\" --max-warnings 0",
    "lint": "npm run lint:prettier && npm run lint:eslint && npm run check-types",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@bem-react/classname": "^1.6.0",
    "axios": "^1.6.8",
    "chalk": "^5.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "http-proxy-middleware": "^2.0.6",
    "lodash.debounce": "^4.0.8",
    "lodash.merge": "^4.6.2",
    "lodash.throttle": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-intersection-observer": "^9.8.1",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "shallowequal": "^1.1.0",
    "sirv": "^2.0.4",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@tsconfig/create-react-app": "^2.0.2",
    "@tsconfig/vite-react": "^3.0.1",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.x",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/lodash.throttle": "^4.1.9",
    "@types/node": "^18.19.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.18",
    "@types/shallowequal": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.8.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "prettier": "^3.2.5",
    "type-fest": "^4.10.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.5"
  }
}
